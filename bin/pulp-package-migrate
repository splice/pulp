import os
import sys



if __name__ == '__main__':
    if os.getuid() != 0:
        print("Error: pulp-package-migrate should be executed as a root user")
        sys.exit(-1)
    from pulp.common import pulp_package_migrate
    sys.exit(pulp_package_migrate.main())

